  "dialogue_blocks": [
    {
      "speaker": "Lucía",
      "text": "¿A qué hora te despiertas normalmente?",
      "translation": "What time do you normally wake up?",
      "ipa": "[a ke ˈoɾa te desˈpjeɾtas noɾˈmalˈmente]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-001",
      "alternate_phrasing": [],
      "context": "Asking about morning routine"
    },
    {
      "speaker": "Marco",
      "text": "Me despierto a las seis y media. Luego me ducho.",
      "translation": "I wake up at six thirty. Then I shower.",
      "ipa": "[me desˈpjeɾto a las ˈsejs i ˈmeðja ‖ ˈlweɣo me ˈðutʃo]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-002",
      "alternate_phrasing": [],
      "context": "Describing morning routine"
    },
    {
      "speaker": "Lucía",
      "text": "¿A qué hora desayunas?",
      "translation": "What time do you have breakfast?",
      "ipa": "[a ke ˈoɾa ðesaˈʝunas]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-003",
      "alternate_phrasing": [],
      "context": "Asking about breakfast time"
    },
    {
      "speaker": "Marco",
      "text": "Desayuno a las siete. Siempre como huevos y fruta.",
      "translation": "I have breakfast at seven. I always eat eggs and fruit.",
      "ipa": "[ðesaˈʝuno a las ˈsjete ‖ ˈsjempɾe ˈkomo ˈweβos i ˈfɾuta]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-004",
      "alternate_phrasing": [],
      "context": "Describing breakfast habits"
    },
    {
      "speaker": "Lucía",
      "text": "¿A qué hora sales al trabajo?",
      "translation": "What time do you leave for work?",
      "ipa": "[a ke ˈoɾa ˈsales al tɾaˈβaxo]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-005",
      "alternate_phrasing": [],
      "context": "Asking about leaving time"
    },
    {
      "speaker": "Marco",
      "text": "Salgo a las ocho menos cuarto. Llego al trabajo en metro.",
      "translation": "I leave at quarter to eight. I get to work by metro.",
      "ipa": "[ˈsalɣo a las ˈotʃo ˈmenos ˈkwaɾto ‖ ˈʝeɣo al tɾaˈβaxo en ˈmetɾo]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-006",
      "alternate_phrasing": [],
      "context": "Stating departure and transport"
    },
    {
      "speaker": "Lucía",
      "text": "¿Y por la noche, ¿a qué hora te acuestas?",
      "translation": "And at night, what time do you go to bed?",
      "ipa": "[i poɾ la ˈnotʃe ‖ a ke ˈoɾa te aˈkwestas]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-007",
      "alternate_phrasing": [],
      "context": "Asking about bedtime"
    },
    {
      "speaker": "Marco",
      "text": "Normalmente me acuesto a las once. Leo un poco antes de dormir.",
      "translation": "I normally go to bed at eleven. I read a bit before sleeping.",
      "ipa": "[noɾˈmalˈmente me aˈkwesto a las ˈonse ‖ ˈleo um ˈpoko ˈantes ðe ðoɾˈmiɾ]",
      "dialect": "es-MX",
      "audio_asset_id": "a1-dr-mx-008",
      "alternate_phrasing": [],
      "context": "Describing evening routine"
    }
  ],
